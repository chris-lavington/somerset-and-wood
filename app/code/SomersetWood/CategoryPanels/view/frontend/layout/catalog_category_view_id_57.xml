<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
      <referenceContainer name="content">
        <referenceContainer name="category.view.container">
          <block class="SomersetWood\CategoryPanels\Block\Panels"
                 name="category.panels" 
                 template="SomersetWood_CategoryPanels::age_panels.phtml"
                 before="-">
   
          </block>
        </referenceContainer>
      </referenceContainer>
      <!-- to move category tiles into main content area -->
      <move element="category.view" destination="content" after="-" />
    </body>
</page>
